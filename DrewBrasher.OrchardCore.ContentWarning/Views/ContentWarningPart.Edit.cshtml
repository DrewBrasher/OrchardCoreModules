@model ContentWarningPartViewModel

<div class="mb-3">
    <div class="form-check">
        <input type="checkbox" class="form-check-input" asp-for="ShowWarning" />
        <label class="form-check-label" asp-for="ShowWarning">@T["Show Warning"]</label>
    </div>
    <div class="form-group">
        <label asp-for="WarningMessage">@T["Warning Message"]</label>
        <input type="text" asp-for="WarningMessage" class="form-control" />
    </div>
</div>
